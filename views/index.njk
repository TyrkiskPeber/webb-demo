{% extends "layout.njk" %}
{% block  content %}
<main>
    <h1>{{ title }}</h1>
    <p>{{ intro }}</p>
    <p>Detta är dock statisk text. Följt av data från databas.</p>
    <ul>
        {% for row in rows %}
        <div class="d-flex">
        <li>Name: {{ row.name }} || Secret Identity: {{row.secret_identity}}
            <br>
            Bio: {{row.bio}}
            <br>
            <br>
        </li>
        {% endfor %}
    </ul>
</main>
{% endblock %}%